#include <bits/stdc++.h>

using namespace std;

int main()
{
	//freopen("input.txt","r",stdin);
	int x1,y1,x2,y2,x,y,a,b;
	cin>>x1>>y1>>x2>>y2>>x>>y;
	int dx=abs(x2-x1);
	int dy=abs(y2-y1);
	if(dx%x==0&&dy%y==0)
	{
		a=dx/x;
		b=dy/y;
		if((a%2==0&&b%2==0)||(a%2==1&&b%2==1))
			cout<<"YES"<<endl;
		else
			cout<<"NO\n";
	}
	else
		cout<<"NO\n";
	return 0;
}
